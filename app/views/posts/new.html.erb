<%= form_with scope: :post, url: posts_path, local: true do |form| %>
  <p>
    <%= form.label :photo %><br>
    <%= form.text_field :photo %>
  </p>

  <p>
    <%= form.label :desc %><br>
    <%= form.text_area :desc %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', posts_path %>
